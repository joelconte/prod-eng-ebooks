<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:stack-db="http://code.lds.org/schema/spring/db"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
						   http://code.lds.org/schema/spring/db http://code.lds.org/schema/spring/db/stack-db-3.0.xsd"
	   default-lazy-init="true">
	<stack-db:data-source url="#{T(org.familysearch.prodeng.settings.Constants).dsUrl}"
						  user="#{T(org.familysearch.prodeng.settings.Constants).dsUser}"
						  password="#{T(org.familysearch.prodeng.settings.Constants).dsPassword}"
						  override="true"
						  db-env="EMBEDDED"/>
</beans>
